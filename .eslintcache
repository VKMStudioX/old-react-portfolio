\nconst App = () =>  {\n  \n    const mainRef = useRef();\n    const projectsRef = useRef();\n    const contactRef = useRef();\n\n    const ScrolltoY = (y) => {\n      window.scrollTo({\n        top: y,\n        left: 0,\n        behavior: \"smooth\"\n      });\n    }\n\n    const executeScroll = (ref) => {\n      if (ref === 'main') {ScrolltoY(mainRef.current.offsetTop-80)}\n      else if(ref ==='projects') { ScrolltoY(projectsRef.current.offsetTop-75)}\n      else if(ref ==='contact') { ScrolltoY(contactRef.current.offsetTop)}\n      else {}\n    }\n\n    const [active, setActive] = useState();\n\n  useEffect(() => {\n    switch (active) {\n         case 'main': \n             return executeScroll('main')\n         case 'projects': \n             return executeScroll('projects')\n         case 'contact': \n             return executeScroll('contact')\n         default:\n             return executeScroll('main')\n         }\n     }, [active]);\n\n\n    return (\n      <>\n      <StoreProvider>\n        <Router>\n        <ScrollToTop>\n        <Segment className='background' padded={false}> \n        <HeaderSui />\n\n          <Switch>\n            <Route exact path='/' render={() => {setActive('main')}} />\n            <Route path='/projects' render={() => {setActive('projects')}} />\n            <Route path='/contact' render={() => {setActive('contact')}} />\n          </Switch>\n\n          <div ref={mainRef}>\n            <SkillsSUI/>\n          </div>\n          \n          <div ref={projectsRef}>\n          <ProjectsSui/>\n          </div>\n\n          <div ref={contactRef}>\n          <ContactSUI/>\n          </div>\n\n          <ModalImageViewer />\n          <ScrollTopArrow />\n          <FooterSui/>\n          </Segment>\n        </ScrollToTop>\n        </Router>\n      </StoreProvider>\n            </>\n      );\n  };\n\nexport default App;","C:\\IMPORTANT\\JAVASCRIPT\\01_REACT\\CreateReactApp\\PortfolioKM\\portfolio_km\\src\\Header.jsx",["103"],"import R